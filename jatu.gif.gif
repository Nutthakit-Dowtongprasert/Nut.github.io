.flash-btn {
  position: relative;
  display: inline-block;
  padding: 14px 34px;
  font-size: 22px;
  font-weight: 700;
  letter-spacing: 1px;
  color: #3326ab; /* text color inside */
  background: #fff; /* fallback bg */
  border-radius: 8px;
  overflow: hidden;
  z-index: 1;
}
.flash-btn:hover {
  color: #3326ab !important;
}


/* animated border effect */
.flash-btn::before {
  content: "";
  position: absolute;
  top: -2px;
  left: -2px;
  right: -2px;
  bottom: -2px;
  background: linear-gradient(
    90deg,
    #3326ab,
    #00c6ff,
    #3326ab
  );
